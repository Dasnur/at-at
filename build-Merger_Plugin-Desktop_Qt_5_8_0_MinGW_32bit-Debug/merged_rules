ИМЯ "Правило_Здоров_ПЗ_2"
ТИП "Обычное"
ЕСЛИ
  (Счётчик.Голоса_за_Здоров + Счётчик.Голоса_за_Болен = 1) &
  (Счётчик.Голоса_за_Здоров>=Счётчик.Голоса_за_Болен)
ТО
  Счётчик.Результат = "Здоров"

ИМЯ "Правило_Болен_ПЗ_2"
